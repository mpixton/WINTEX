@{
    ViewData["Title"] = "Datums";
}

@model WINTEX.Models.Gisdatum



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>

    <div>
        <h4>Gisdatum</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.MaturityCode)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MaturityCode)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.WrappingCode)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.WrappingCode)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Gisid)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Gisid)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Mummy)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Mummy.MummyId)
            </dd>
        </dl>
    </div>
    <div>
        @if (User.IsInRole("Admin") || User.IsInRole("Researcher"))
        {
            <div class="p-1">
                <a class="btn btn-admin" asp-action="Edit" asp-route-id="@Model.MummyId">Edit</a>

            </div>
        }
        <div class="p-1">
            <a class="btn btn-admin" asp-action="Index">Back to List</a>

        </div>

    </div>
</body>
</html>
