@{
    ViewData["Title"] = "Burial Storage";
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}

@model WINTEX.Models.FegbmummyStorage



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit - Burial Storage</title>
</head>
<body>
    <form asp-action="Edit">
        <div class="d-flex justify-content-center">
            
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="MummyId" />
                <input type="hidden" asp-for="ShelfId" />
            
        </div>
        <div class="d-flex">
            <div class="d-flex justify-content-end mr-3" style="width: 50%">
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>
            </div>
            <div class="d-flex justify-content-start ml-3" style="width: 50%">
                <div>
                    <a class="btn btn-admin" asp-action="Index">Back to List</a>
                </div>
            </div>
        </div>
    </form>
    
    @section Scripts {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }
</body>
</html>
