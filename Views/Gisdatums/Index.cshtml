@model IEnumerable<WINTEX.Models.Gisdatum>



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MaturityCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WrappingCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gisid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mummy)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaturityCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WrappingCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gisid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mummy.MummyId)
            </td>
            <td>
                @if (User.IsInRole("Admin")) 
                { 
                    <a asp-action="Edit" asp-route-id="@item.MummyId">Edit</a>
                    <a asp-action="Delete" asp-route-id="@item.MummyId">Delete</a>
                }
                <a asp-action="Details" asp-route-id="@item.MummyId">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
