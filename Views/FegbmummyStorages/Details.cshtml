@{
    ViewData["Title"] = "Burial Storage";
    Layout = "~/Views/Shared/_DetailsLayout.cshtml";
}

@model WINTEX.Models.FegbmummyStorage


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>

    <div class="d-flex justify-content-center">
        <dl class="row">
            <dt class="col-sm-5">
                @Html.DisplayNameFor(model => model.Mummy)
            </dt>
            <dd class="col-sm-7">
                @Html.DisplayFor(model => model.Mummy.MummyId)
            </dd>
            <dt class="col-sm-5">
                @Html.DisplayNameFor(model => model.Shelf)
            </dt>
            <dd class="col-sm-7">
                @Html.DisplayFor(model => model.Shelf.Shelf)
            </dd>
        </dl>
    </div>
    <div class="d-flex flex-row justify-content-center mt-4">
        @if (User.IsInRole("Admin") || User.IsInRole("Researcher"))
        {
            <div class="d-flex p-1 mr-1">
                @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ })
            </div>
        }
        <div class="d-flex p-1 ml-1">
            <a class="btn btn-admin" asp-action="Index">Back to List</a>
        </div>
    </div>
</body>
</html>
