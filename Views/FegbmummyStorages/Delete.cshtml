@{
    ViewData["Title"] = "Burial Storage";
    Layout = "~/Views/Shared/_DetailsLayout.cshtml";
}

@model WINTEX.Models.FegbmummyStorage


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <div class="d-flex justify-content-center">
        <h3>Are you sure you want to delete this?</h3>
    </div>
    <hr />
    <br />
    <div class="d-flex justify-content-center">
        <dl class="row">
            <dt class="col-sm-5">
                @Html.DisplayNameFor(model => model.Mummy)
            </dt>
            <dd class="col-sm-7">
                @Html.DisplayFor(model => model.Mummy.MummyId)
            </dd>
            <dt class="col-sm-5">
                @Html.DisplayNameFor(model => model.Shelf)
            </dt>
            <dd class="col-sm-7">
                @Html.DisplayFor(model => model.Shelf.Shelf)
            </dd>
        </dl>
    </div>
    <div class="d-flex justify-content-center">
        <form asp-action="Delete">
            <input type="hidden" asp-for="MummyId" />
            <input type="hidden" asp-for="ShelfId" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a class="btn btn-admin" asp-action="Index">Back to List</a>
        </form>
    </div>
</body>
</html>
